{
  "private": true,
  "version": "v1.0.0",
  "scripts": {
    "test:unit": "env-cmd -e local jest server",
    "build": "npm run build:infra && npm run build:server && distize dist-server/* package.json",
    "build:infra": "rimraf ./dist-infra && tsc --build tsconfig.infra.json",
    "build:server": "rimraf ./dist-server && tsc --build tsconfig.server.json",
    "deploy:dev": "npm run build && env-cmd -e dev cdk deploy",
    "deploy:prod": "npm run build && env-cmd -e prod cdk deploy",
    "dev": "env-cmd -e local nodemon --exec \"npm run build:server && node dist-server/entries/dev.js\""
  },
  "dependencies": {
    "@graphity/server-lambda": "^0.6.3",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-server-core": "^2.13.1",
    "aws-sdk": "^2.768.0",
    "axios": "^0.19.2",
    "core-js": "^3.6.0",
    "dataloader": "^1.4.0",
    "date-fns": "^2.16.1",
    "dynamom": "^0.3.6-alpha",
    "graphity": "^0.7.0",
    "graphql": "^14.6.0",
    "htmlparser2": "^6.0.0",
    "qs": "^6.9.4"
  },
  "devDependencies": {
    "@aws-cdk/aws-apigateway": "^1.66.0",
    "@aws-cdk/aws-events-targets": "^1.66.0",
    "@aws-cdk/aws-lambda": "^1.66.0",
    "@aws-cdk/aws-logs": "^1.66.0",
    "@aws-cdk/aws-route53-targets": "^1.66.0",
    "@aws-cdk/core": "^1.66.0",
    "@graphity/server-express": "^0.6.3",
    "@stdjs/eslint-config": "^2.0.0-alpha.2",
    "@types/aws-lambda": "^8.10.23",
    "@types/express": "^4.17.6",
    "@types/graphql": "^14.5.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^12.12.21",
    "aws-cdk": "^1.66.0",
    "cpx": "^1.5.0",
    "distize": "^1.2.0",
    "env-cmd": "^10.1.0",
    "eslint": "^6.2.0",
    "eslint-config-stable": "^0.8.0",
    "jest": "^26.5.2",
    "nodemon": "^2.0.4",
    "rimraf": "^3.0.2",
    "source-map-support": "^0.5.19",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.3"
  }
}
